{
    "name": "merchify",
    "version": "0.0.7",
    "description": "Merchify Javascript SDK",
    "private": false,
    "type": "module",
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist/**"
    ],
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "clean": "rm -rf node_modules && rm -rf dist",
        "lint": "eslint .",
        "typecheck": "tsc --project ./tsconfig.json --noEmit",
        "test": "vitest run"
    },
    "devDependencies": {
        "@types/node": "^20.11.24",
        "eslint": "^9.0.0",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3",
        "vitest": "^1.0.0"
    }
}